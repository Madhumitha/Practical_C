#include <stdio.h>
//int width = 3; 
float width = 6.8;
float height = 2.3;
//int height = 5;
int area, perimeter;
int main()
{
area = width * height;
perimeter = 2*(width + height);
printf(" The area of the rectangle is %d\n", area);
printf(" The perimeter of the rectangle is %d\n", perimeter);
return (0);
}


